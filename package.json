{
  "name": "llm-kit",
  "version": "1.0.1",
  "description": "A Node.js module for searching and scraping web content, designed for LLMs but useful for any project where webscraping is needed!",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "prepare": "npm run build",
    "test": "ts-node src/test.ts",
    "test:ecosia": "ts-node src/test-ecosia.ts",
    "test:google": "ts-node src/test-google.ts",
    "test:ddg": "ts-node src/test-ddg.ts",
    "lint": "eslint src/**/*.ts",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run test && npm run build"
  },
  "keywords": [
    "search",
    "scraping",
    "llm",
    "ai",
    "wikipedia",
    "hackernews",
    "readability",
    "google",
    "duckduckgo",
    "typescript",
    "web-scraping",
    "content-extraction",
    "llm-scrape",
    "web-search",
    "scrape",
    "web-scrape",
    "web-crawl",
    "crawling",
    "web-crawler"
  ],
  "author": {
    "name": "Minoa",
    "url": "https://github.com/M1noa"
  },
  "license": "MIT",
  "dependencies": {
    "@mozilla/readability": "^0.6.0",
    "cheerio": "^1.0.0",
    "chrome-lens-ocr": "^4.1.0",
    "csv-parse": "^5.6.0",
    "duck-duck-scrape": "^2.2.7",
    "fast-xml-parser": "^5.2.3",
    "google-news-scraper": "^2.7.0",
    "google-sr": "^5.0.0",
    "jsdom": "^26.1.0",
    "llm-kit": "file:",
    "mammoth": "^1.9.1",
    "pdf-parse": "^1.1.1",
    "rss-parser": "^3.13.0",
    "turndown": "^7.2.0",
    "wikipedia": "^2.1.2",
    "yahoo-finance2": "^2.13.3"
  },
  "devDependencies": {
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.15.29",
    "@types/pdf-parse": "^1.1.5",
    "@types/turndown": "^5.0.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  },
  "engines": {
    "node": ">=16"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/M1noa/llm-kit.git"
  },
  "bugs": {
    "url": "https://github.com/M1noa/llm-kit/issues"
  },
  "homepage": "https://github.com/M1noa/llm-kit#readme",
  "files": [
    "dist",
    "LICENSE",
    "README.md",
    "docs"
